<header class="navbar navbar-fixed-top navbar-inverse header-option">
    <div class="container">
    <nav>
        <ul class="nav navbar-nav navbar-left">
        <h1><%= link_to "GetCoder", root_path, class: "header-title" %></h1>
        </ul>
        <ul class="nav navbar-nav navbar-right">
        <li><%= link_to "Home", root_path %></li>
        <li><%= link_to "Help", help_path %></li>
        <% if logged_in? %>
            <%= link_to "Logout", logout_path, data: { "turbo-method": :delete } %>
            <li><%= button_to '削除', logout_path, class: 'btn btn-outline-dark', method: :delete %></li>
        <% else %>
            <li><%= link_to "Login", login_path %></li>
            <li><%= link_to "Signup", signup_path %></li>
        <% end %>
        </ul>
    </nav>
    </div>
</header>
